<%= partial('html') %>
<head>
  <%= partial('head') %>
</head>
<%= partial('body') %>
  <%= partial('organisms/header') %>

  <main class="main" id="__main" data-import="search">
    <% if (pages.search.top) { %>
      <section class="rounded p-3">
        <%= md.render(pages.search.top) %>
      </section>
    <% } %>
    <div class="container search py-1 py-sm-2 py-lg-4">
      <div
        class="_ecom-el"
        data-type="items"
        data-vm="Retail"
        data-term=" "
        data-from="0"
        data-size="20">
        <%= partial('organisms/retail', { info: true }) %>
      </div>
    </div>
    <% if (pages.search.botton) { %>
      <section class="rounded p-3">
        <%= md.render(pages.search.botton) %>
      </section>
    <% } %>
  </main>

  <%= partial('organisms/footer') %>
</body>
</html>
